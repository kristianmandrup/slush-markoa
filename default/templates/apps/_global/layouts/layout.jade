lasso-page(name="$data.name" package-path="./${data.name}.browser.json")
doctype html
html
  head
    link(rel="stylesheet" type="text/css" href="semantic.min.css")
    script(src="semantic/dist/semantic.min.js")    
    block title
      title Default title
    lasso-head
  body
    block content
      h1 Default page
    lasso-body
    init-widgets
    browser-refresh(enabled="true")
