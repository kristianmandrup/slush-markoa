.item $data.label
if(test='$data.icon')
  i(class='$data.icon icon')
if(test='$data.content')
.content
  if(test='$data.link')
    a(href='$data.link.url') ${data.link.label || data.link.url}
  else
    if(test='$data.header')
      a.header $data.header
    if(test='$data.desc')
      $data.desc
    if(test='$data.content')
      $data.content
